/* pages/cartoon/cartoon.wxss */

.nav {
  height: 140rpx;
  background-color: rgba(0,0,0,0.9);
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}

.nav image {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0rpx;
  margin: auto;
  z-index: 999;
}